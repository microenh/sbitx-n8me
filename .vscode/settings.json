  "files.associations": {
    "*.c": "c",
    "*.h": "c",
    "fields.h": "c",
    "typeinfo": "c"
},
  "cmake.buildDirectory": "${workspaceFolder}/builder",
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
  "cmake.configureOnOpen": true,
  "cmake.preferredGenerators": [
    "ninja"
  ],
  "editor.detectIndentation": false
}